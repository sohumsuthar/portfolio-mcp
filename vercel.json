{
  "name": "portfolio-mcp",
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["sfo1"],
  "functions": {
    "dist/api-server.js": {
      "memory": 512,
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/.*",
      "dest": "dist/api-server.js"
    }
  ]
}
